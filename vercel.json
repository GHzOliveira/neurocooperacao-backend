{
    "version": 2,
    "name": "neurocoperacao-backend",
    "builds": [
      {
        "src": "dist/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ],
    "outputDirectory": "dist"
  }